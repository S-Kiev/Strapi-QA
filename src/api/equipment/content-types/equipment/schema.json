{
  "kind": "collectionType",
  "collectionName": "equipments",
  "info": {
    "singularName": "equipment",
    "pluralName": "equipments",
    "displayName": "equipment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "equipmentId": {
      "type": "uid",
      "required": false
    },
    "name": {
      "type": "string",
      "required": true
    },
    "brand": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": false
    },
    "deactivationDate": {
      "type": "datetime"
    },
    "availabilityStatus": {
      "type": "enumeration",
      "enum": [
        "available",
        "rented",
        "broken",
        "out of use"
      ],
      "default": "available",
      "required": true
    }
  }
}
